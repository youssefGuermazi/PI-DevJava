<AnchorPane id="AnchorPane" prefHeight="315.0" prefWidth="553.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pi.devjava.controller.AjoutSuiviController">
   <children>
      <JFXTextField fx:id="fr" layoutX="51.0" layoutY="96.0" onKeyReleased="#frChange" />
