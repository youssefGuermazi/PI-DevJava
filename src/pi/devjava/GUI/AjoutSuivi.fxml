      <Label layoutX="211.0" layoutY="132.0" text="date" visible="false" />
      <ToolBar layoutX="-3.0" prefHeight="40.0" prefWidth="560.0">
         <items>
            <Button fx:id="ajout" mnemonicParsing="false" onAction="#AjoutSuivi" prefHeight="25.0" prefWidth="104.0" text="Ajout">
